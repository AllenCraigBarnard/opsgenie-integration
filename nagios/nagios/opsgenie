#!/bin/sh

if [ "$1" = "service" ]
then
lamp executeScript --name ogCreateAlert.groovy -Dentity="$1" -DNOTIFICATIONTYPE="$2" -DHOSTADDRESS="$3" -DLONGDATETIME="$4" -DHOSTNAME="$5" -DHOSTALIAS="$6" -DSERVICEDESC="$7" -DSERVICESTATE="$8" -DSERVICEOUTPUT="$9" -DSERVICEGROUPNAME="$10" -DLASTSERVICECHECK="$11" -DLASTSERVICESTATECHANGE="$12" -DSERVICEDURATION="$13" -DSERVICEATTEMPT="$14" -DMAXSERVICEATTEMPTS="$15" -DSERVICESTATETYPE="$16" -DSERVICELATENCY="$17" -DSERVICECHECKTYPE="$18" -DSERVICEPERFDATA="$19"
else
lamp executeScript --name ogCreateAlert.groovy -Dentity="$1" -DNOTIFICATIONTYPE="$2" -DHOSTADDRESS="$3" -DLONGDATETIME="$4" -DHOSTNAME="$5" -DHOSTALIAS="$6" -DHOSTGROUPNAME="$7" -DHOSTSTATE="$8" -DLASTHOSTCHECK="$9" -DLASTHOSTSTATECHANGE="$10" -DHOSTDURATION="$11" -DHOSTOUTPUT="$12" -DHOSTATTEMPT="$13" -DMAXHOSTATTEMPTS="$14" -DHOSTSTATETYPE="$15" -DHOSTLATENCY="$16" -DHOSTCHECKTYPE="$17" -DHOSTPERFDATA="$18"
fi

